{
    "name": "flowndly-cpanel",
    "version": "1.0.0",
    "description": "Flowndly - Workflow Automation Platform for cPanel",
    "main": "src/index.js",
    "scripts": {
        "start": "node src/index.js",
        "dev": "nodemon src/index.js",
        "migrate": "knex migrate:latest",
        "migrate:rollback": "knex migrate:rollback",
        "seed": "knex seed:run",
        "seed:user": "knex seed:run --specific=01-user.js",
        "db:migrate": "knex migrate:latest",
        "db:seed:user": "knex seed:run --specific=01-user.js",
        "start:worker": "node src/worker.js"
    },
    "dependencies": {
        "@automatisch/types": "0.15.0",
        "automatisch": "0.15.0",
        "express": "^4.18.2",
        "knex": "^3.0.1",
        "objection": "^3.0.1",
        "pg": "^8.11.3",
        "redis": "^4.6.10",
        "dotenv": "^16.3.1",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "compression": "^1.7.4",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "jsonwebtoken": "^9.0.2",
        "bcryptjs": "^2.4.3",
        "multer": "^1.4.5-lts.1",
        "uuid": "^9.0.1",
        "moment": "^2.29.4",
        "lodash": "^4.17.21",
        "axios": "^1.6.0",
        "node-cron": "^3.0.3",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.2"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "keywords": [
        "workflow",
        "automation",
        "cpanel",
        "nodejs",
        "postgresql",
        "redis"
