{
  "version": 2,
  "name": "flowndly",
  "builds": [
    {
      "src": "packages/backend/src/vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/packages/backend/src/vercel.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "APP_ENV": "production"
  },
  "functions": {
    "packages/backend/src/vercel.js": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}